{
  "name": "aws-multi-region-cdk-demo",
  "version": "1.0.0",
  "description": "Multi-region AWS CDK demonstration for IoT and Payment applications",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "npm run build && cdk deploy",
    "deploy:all": "npm run build && cdk deploy --all",
    "destroy": "cdk destroy",
    "destroy:all": "cdk destroy --all",
    "synth": "cdk synth",
    "diff": "cdk diff",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.6.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.176.0",
    "constructs": "^10.0.0",
    "cdk-nag": "^2.27.0"
  },
  "keywords": [
    "aws",
    "cdk",
    "multi-region",
    "iot",
    "payment",
    "infrastructure"
  ],
  "author": "AWS CDK Demo",
